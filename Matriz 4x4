import numpy as matriz
n_array =  matriz.array ([[1,7,29,83],
                        [2,3,11,37],
                        [8,-9,13,41],
                        [16,11,37,-53]])
print("matriz 4x4 :")
print(n_array)
det = matriz.linalg.det(n_array)
print("\nDeterminante da matriz Ã© :")
print(int(det))

